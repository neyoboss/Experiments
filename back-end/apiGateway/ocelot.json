{
    "Routes": [
        {
            "DownstreamPathTemplate": "/api/profile/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "profile-service",
                    "Port": 80
                }
            ],
            "AuthenticationOptions": {
                "AuthenticationProviderKey": "Auth0"
            },
            "UpstreamPathTemplate": "/api/profile/{everything}"
        },
        {
            "DownstreamPathTemplate": "/api/matches/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "match-service",
                    "Port": 80
                }
            ],
            "UpstreamPathTemplate": "/api/matches/{everything}"
        },
        {
            "DownstreamPathTemplate": "/api/auth/{everything}",
            "UpstreamPathTemplate": "/api/auth/{everything}",
            "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
              {
                "Host": "auth-service",
                "Port": 80
              }
            ]
          }
    ],
    "GlobalConfiguration": {
        "AuthenticationOptions": {
            "AuthenticationProviderKey": "Auth0"
        },
        "BaseUrl": "http://+:8000"
    }
}